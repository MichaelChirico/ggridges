> set.seed(10)
> n = 20
> data.context1 = tibble::tibble(x1 = rnorm(n, -3),
+                                    x2 = rnorm(n,  3),
+                                    z  = sample(1:3, n, replace=TRUE),
+                                    y  =I(z==1) * (3 + 4*x1 - x2 + rnorm(n)) +
+                                        I(z==2) * (3 + 2*x1 + x2 + rnorm(n)) +
+                                        I(z==3) * (3 - 4*x1 - x2 + rnorm(n)) ,
+                                    w = 20
+                                    ) 
> data.context2 = tibble::tibble(x1 = rnorm(n, -3),
